{
  "manifest_version": 3,
  "name": "Keno Stats",
  "version": "1.0",
  "description": "Tracks Keno history and provides number stats on Stake.",
  "permissions": [
    "storage",
    "management"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://stake.com/*",
        "https://stake.us/*"
      ],
      "js": [
        "dist/content.bundle.js"
      ],
      "run_at": "document_idle"
    },
    {
      "matches": [
        "https://stake.com/*",
        "https://stake.us/*"
      ],
      "js": [
        "interceptor.js"
      ],
      "world": "MAIN",
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "betbook.html",
        "betbook.js",
        "disclaimer.html",
        "disclaimer.js",
        "config/bet-multis.json"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}